{
    "name": "fezui",
    "version": "1.0.1",
    "description": "fezui 是一套基于 Vue 的支持多终端的开源UI交互组件库，致力于积木式的快速构建项目，提升产品体验和开发效率、降低开发和维护成本。",
    "main": "dist/js/fezui.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/furic-zhao/fezui"
    },
    "scripts": {
        "dist:style": "gulp",
        "dist:dev": "webpack --config webpack.config.babel.js",
        "dist:min": "webpack --config webpack.config.min.babel.js",
        "dist": "npm run dist:style && npm run dist:dev && npm run dist:min"
    },
    "author": "furic zhao",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel": "^6.23.0",
        "babel-core": "^6.25.0",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.5.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-runtime": "^6.23.0",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.4",
        "gulp": "git://github.com/gulpjs/gulp.git#4.0",
        "gulp-autoprefixer": "^4.0.0",
        "gulp-clean-css": "^3.4.1",
        "gulp-less": "^3.3.0",
        "gulp-rename": "^1.2.2",
        "less": "^2.7.2",
        "less-loader": "^4.0.4",
        "vue-loader": "^12.2.1",
        "vue-style-loader": "^3.0.1",
        "webpack": "^2.6.1",
        "webpack-merge": "^4.1.0"
    },
    "dependencies": {}
}
